            <value>
                <li Class="MoreFactionInteraction.World_Incidents.WorldObjectCompProperties_BumperCrop" />
            </value>
    </Operation>

    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/WorldObjectDef[defName="Caravan"]</xpath>
