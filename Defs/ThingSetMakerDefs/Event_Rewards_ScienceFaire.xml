<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Nothing to translate in here. -->

    <ThingSetMakerDef>        <!-- a buff, gizmo, possibly one of the special buildings, and maaaaybe an artefact. -->
        <defName>Reward_scienceFaireFirstPlace</defName>
        <root Class="ThingSetMaker_Sum">
            <fixedParams>
                <techLevel>Industrial</techLevel>
            </fixedParams>
            <options>
                <li>
                    <chance>0.95</chance>
                    <thingSetMaker Class="ThingSetMaker_Count">
                        <fixedParams>
                            <qualityGenerator>Reward</qualityGenerator>
                            <filter>
                                <thingDefs>
                                    <li>MechSerumHealer</li>
                                    <li>MechSerumResurrector</li>
                                    <li>MechSerumNeuroTrainer</li>
                                    <li>TechprofSubpersonaCore</li>
                                </thingDefs>
                            </filter>
                            <countRange>
                                <min>0</min>
                                <max>1</max>
                            </countRange>
                        </fixedParams>
                    </thingSetMaker>
                </li>
                <li>
                    <chance>0.05</chance>
                    <thingSetMaker Class="ThingSetMaker_Count">
                        <fixedParams>
                            <techLevel>Industrial</techLevel>
                            <filter>
                                <categories>
                                    <li>Artifacts</li>
                                </categories>
                            </filter>
                            <countRange>
                                <min>0</min>
                                <max>2</max>
                            </countRange>
                        </fixedParams>
                    </thingSetMaker>
                </li>
                <li>
                    <chance>0.1</chance>
                    <thingSetMaker Class="ThingSetMaker_StackCount">
                        <fixedParams>
                            <techLevel>Industrial</techLevel>
                            <filter>
                                <thingDefs>
                                    <li>Luciferium</li>
                                </thingDefs>
                            </filter>
                            <countRange>
                                <min>5</min>
                                <max>20</max>
                            </countRange>
                        </fixedParams>
                    </thingSetMaker>
                </li>
            </options>
        </root>
    </ThingSetMakerDef>

